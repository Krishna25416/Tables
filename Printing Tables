#include <stdio.h>

int main() {
    int number, i;

    // Ask user for input
    printf("Enter a number to display its multiplication table: ");
    scanf("%d", &number);

    // Print multiplication table
    printf("Multiplication table for %d:\n", number);
    for (i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", number, i, number * i);
    }

    return 0;
}
